<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="io.boden.MyFinances">
  <!-- project files generated manually with android studio
  would also have the following <application> attributed:
  android:roundIcon="@mipmap/ic_launcher_round"
  android:supportsRtl="true" -->

  

  <application
    android:allowBackup="true"
    android:label="@string/app_name"
    android:theme="@style/Theme.AppCompat.DayNight"
    android:icon="@mipmap/ic_launcher_png">
    <activity android:name="io.boden.android.NativeRootActivity"
              android:label="@string/app_name"
              android:configChanges="mcc|mnc|locale|touchscreen|keyboard|keyboardHidden|navigation|screenLayout|fontScale|uiMode|orientation|screenSize|smallestScreenSize|layoutDirection">

      <meta-data android:name="io.boden.android.lib_name" android:value="MyFinances" />

      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>         
  </application>
</manifest>
